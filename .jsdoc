{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "source": {
    "include": "./src",
    "includePattern": ".js",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "encoding": "utf8",
    "destination": "build/docs/",
    "recurse": true,
    "verbose": true,
    "private": false
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
